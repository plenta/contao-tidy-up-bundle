{
  "name": "plenta/contao-tidy-up-bundle",
  "type": "contao-bundle",
  "description": "Tidy up your contao projects",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Plenta.io",
      "homepage": "https://plenta.io"
    }
  ],
  "funding": [{
    "type": "github",
    "url": "https://github.com/sponsors/plenta"
  }],
  "support":{
    "issues": "https://github.com/plenta/contao-tidy-up-bundle/issues",
    "source": "https://github.com/plenta/contao-tidy-up-bundle"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/conflicts": "@dev",
    "contao/core-bundle": "^4.4 || ^4.9 || ^4.13"
  },
  "require-dev": {
    "contao/manager-bundle": "4.4.57",
    "phpunit/phpunit": "^9.5.10",
    "contao/test-case": "^4.4.0",
    "symfony/phpunit-bridge": "v5.3.8",
    "friendsofphp/php-cs-fixer": "^2.19"
  },
  "extra": {
    "contao-manager-plugin": "Plenta\\ContaoTidyUpBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Plenta\\ContaoTidyUpBundle\\": "src/Plenta/ContaoTidyUpBundle"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "contao-components/installer": true,
      "contao/manager-plugin": true,
      "contao-community-alliance/composer-plugin": true
    }
  }
}
